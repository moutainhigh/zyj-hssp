        function BPM102_tagEditorFunction(record, name) {
            var tagType = record.get('tagType');

            if (record.get('templateFlag') == 'N') {
                if (name == 'tagSequence') {
                    return 'BPM102_tagNf';
                } else if (name == 'tagCode') {
                    return 'BPM102_tagTf';
                } else if (name == 'tagTypeDesc') {
                    return 'BPM102_tagCb';
                } else if (name == 'name' && tagType != 'merge_column') {
                    return 'BPM102_tagLov';
                }
            }

            if (name == 'readonly' && tagType != 'merge_column') {
                return 'BPM102_tagCk';
            } else if (name == 'required' && tagType != 'merge_column') {
                return 'BPM102_tagCk';
            } else if (name == 'defaultvalue' && tagType != 'merge_column') {
                return 'BPM102_tagTa';
            }

            var layoutType = '$[/parameter/@layoutType]';
            if ((layoutType == 'form' && tagType != 'merge_column') || layoutType == 'fieldSet' || layoutType == 'box') {
                if (name == 'height') {
                    return 'BPM102_tagNf';
                } else if (name == 'colspan') {
                    return 'BPM102_tagNf';
                } else if (name == 'rowspan') {
                    return 'BPM102_tagNf';
                }
            } else if (layoutType == 'grid') {
                if (name == 'alignDesc') {
                    return 'BPM102_tagCb';
                } else if (name == 'footerrenderer') {
                    return 'BPM102_tagCk';
                } else if (name == 'lockFlag') {
                    return 'BPM102_tagCk';
                } else if (name == 'sortable' && tagType != 'merge_column') {
                    return 'BPM102_tagCk';
                } else if (name == 'parentTagDesc') {
                    return 'BPM102_tagLov';
                }
            }

            return '';
        }

