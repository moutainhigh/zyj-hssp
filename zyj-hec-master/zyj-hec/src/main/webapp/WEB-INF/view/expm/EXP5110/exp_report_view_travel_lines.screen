                    var vatInvoiceFlag = headerRecord.get('vatInvoiceFlag');
                    if (vatInvoiceFlag == 'Y') {
                        var records = $au('EXP5110_exp_report_travel_line_ds').getAll();
                        for (var j = 0;j < records.length;j++) {
