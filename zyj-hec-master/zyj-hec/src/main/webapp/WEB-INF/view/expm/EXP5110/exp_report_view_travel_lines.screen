                    } else {
                        var data = {};
                        var lineDs = $au('EXP5110_exp_report_travel_line_ds');
                        headerRecord = $au('EXP5110_exp_report_header_ds').getAt(0);
                        data['reportPageElementCode'] = 'TRAVEL_LINES';
                        data['lineNumber'] = EXP5110_travelGetNextLineNumber();
