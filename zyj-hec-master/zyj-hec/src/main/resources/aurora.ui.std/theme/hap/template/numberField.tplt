<DIV class="$[wrapClass] item-tf" isgrid="$[isgrid]" style="width:$[width]px;height:$[height]px;$[style]" id="$[id]" >
	<DIV class="item-input-wrap" style="padding-right:3px;">
		<INPUT placeholder="$[placeholder]" name="$[name]" tabindex="$[tabindex]" style="height:$[height]px;line-height:$[height]px;ime-mode:disabled;$[fontstyle]" type="$[inputtype]" class="item-textField item-numberField" atype="field.input" $[readonly] autocomplete="off" value="$[value]" />
	</DIV>
	<DIV class='tipIcon' style='width:15px;height:15px;float:right;margin-top:8px;display:none;'></DIV>
</DIV>
<script>
	new Aurora.NumberField($[config]);
	$[binding]
</script>