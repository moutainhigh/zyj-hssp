<DIV class="$[wrapClass]" id="$[id]" style="width:$[width]px;$[style]">
	<DIV>$[items]</DIV>
</DIV>
<SCRIPT LANGUAGE="JavaScript">
	new Aurora.NavBar({
		id:'$[id]',
		dataSet:'$[dataset]',
		inputId:'$[inputid]',
		comboBoxId:'$[comboBoxId]',
		width:$[width],
		type:'$[navbartype]',
		maxPageCount:$[maxpagecount]
	});
</SCRIPT>