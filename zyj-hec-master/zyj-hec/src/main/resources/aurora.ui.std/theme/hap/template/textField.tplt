<DIV class="item-tf $[wrapClass]" isgrid="$[isgrid]" style="width:$[width]px;height:$[height]px;$[style]" id="$[id]" >
	<DIV class="item-input-wrap">
		<INPUT placeholder="$[placeholder]" name="$[name]" tabindex="$[tabindex]" style="height:$[height]px;$[lineHeight]text-indent:4px;$[fontstyle]" placeholder="$[placeholder]" type="$[inputtype]" class="item-textField" atype="field.input" $[readonly] autocomplete="off" value="$[value]" />
	</DIV>
	<DIV class='tipIcon' style='width:15px;height:15px;float:right;margin-top:8px;display:none;'></DIV>
</DIV>
<script>
	new Aurora.TextField($[config]);
	$[binding]
</script>